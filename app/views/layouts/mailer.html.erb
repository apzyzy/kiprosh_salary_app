<!DOCTYPE html>
<html>
  <head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <style>
      body{
        font-size: 10px;
      }
      .bold{
        font-weight: bold;
      }
      .big_font{
        font-size: 12px;
      }
      .bigger_font{
        font-size: 14px;
      }
      .small_font{
        font-size: 9px;
      }
      .smaller_font{
        font-size: 8px;
      }
      .width5{
        width: 5%;
      }
      .width10{
        width: 10%;
      }
      .width20{
        width: 20%;
      }
      .width30{
        width: 30%;
      }
      .width50{
        width: 50%;
      }
      .width80{
        width: 80%;
      }
      .width100{
        width: 100%;
      }
      .center_align{
        text-align: center;
      }
      .left_align{
        text-align: left;
      }
      .right_align{
        text-align: right;
      }
      .float_right{
        float: right;
      }
      .float_left{
        float: left;
      }
      .kiprosh_logo{
        display: block;
        margin: auto;
      }
      .main_content{
        width: 90%;
        margin: auto;
      }
      table, th, td{
        border-collapse: collapse;
      }
      td{
        border-bottom: 1px solid #000;
        border-right: 1px solid #000;
        padding: 2px;
      }
      tr td:first-child{
        border-left: 1px solid #000;
      }
      .border_top{
        border-top: 1px solid #000;
      }
      .border_left{
        border-left: 1px solid #000;
      }
      .blank_row{
        border: 0 !important;
      }
      .blue_background{
        background-color: #117ee5;
        color: #FFF;
      }
    </style>
  </head>

  <body>
    <%= yield %>
  </body>
</html>
